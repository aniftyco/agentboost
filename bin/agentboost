#!/usr/bin/env node
import { AgentBoost } from '../dist/index.js';
import plugins from '../dist/plugins/index.js';

const agentboost = new AgentBoost(process.cwd());

await Promise.all(plugins.map((Plugin) => agentboost.register(Plugin)));

agentboost.run(process.argv.slice(2)).catch((err) => {
  console.error(err);
  process.exit(1);
});
